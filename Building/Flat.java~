public class Flat{


		  public int flat_no;
                  public String name;
                 // public int fl_no;
                  
                 
		   
                 public  Flat(String n,int f)
                   
			{ 
                           name=n;
			   flat_no=f;	
                        }
                public void display()
                {
                 System.out.println("Owner name:"+name);
                 System.out.println("Flat No."+flat_no);
		}
}


